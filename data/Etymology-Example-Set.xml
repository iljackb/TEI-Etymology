         <!-- Inheritance with (unlabeled) phonological changes-->
         <entry xml:id="chef" xml:lang="fr">
            <form type="lemma">
               <orth>chef</orth>
               <pron notation="ipa">ʃɛf</pron>
               <gramGrp>
                  <pos>noun</pos>
                  <gen>masc</gen>
               </gramGrp>
            </form>
            <sense><!-- sense info here --></sense>
            <etym type="inheritance">
               <cit type="etymon" xml:id="kápŭ" next="#kabu">
                  <form>
                     <pron notation="proprietary" xml:lang="la-x-VL">kápŭ</pron>
                  </form>
               </cit>
               <cit type="etymon" xml:id="kabu" prev="#kápŭ">
                  <!-- (phonetic process: could label in nested desired; e.g. <etym type="intervocalic voicing"> -->
                  <date notBefore="0350" notAfter="0399"/>
                  <form>
                     <pron notation="proprietary">kábu</pron>
                  </form>
                  <!-- gallo-latin (no iso code)-->
               </cit>
               <cit type="etymon" xml:id="k̯áβo̥" prev="#kabu" next="t̯áβo̥">
                  <date notBefore="0400" notAfter="0499"/>
                  <form>
                     <pron notation="proprietary">k̯áβo̥</pron>
                  </form>
                  <!-- late gallo-latin ?/early gallo-romance-->
               </cit>
               <cit type="etymon" xml:id="t̯ávo̥" prev="#k̯áβo̥" next="t͡sávo̥">
                  <date notBefore="0400" notAfter="0499"/>
                  <form>
                     <pron notation="proprietary">t̯áβo</pron>
                  </form>
                  <!-- late gallo-latin ?/early gallo-romance-->
               </cit>
               <cit type="etymon" xml:id="t͡sávo̥" prev="#t̯ávo̥" next="#t͡šíe̥vo̥">
                  <date notBefore="0400" notAfter="0499"/>
                  <form>
                     <pron notation="proprietary">t͡sávo̥</pron>
                  </form>
                  <!-- late gallo-latin ?/early gallo-romance-->
               </cit>
               <cit type="etymon" xml:id="t͡šíe̥vo̥" prev="#t͡šávo̥" next="#tšíe̥f">
                  <date notBefore="0450" notAfter="0550"/>
                  <form>
                     <pron notation="proprietary">t͡šíe̥vo̥</pron>
                  </form>
                  <!-- early gallo romance -->
               </cit>
               <cit type="etymon" xml:id="tšíe̥f" prev="t͡šíe̥vo" next="#šye̥f">
                  <date notBefore="0600" notAfter="0699"/>
                  <form>
                     <pron notation="proprietary">tšíe̥f</pron>
                  </form>
                  <!-- early gallo romance ?-->
               </cit>
               <cit type="etymon" xml:id="šyé̥f" prev="#tšíe̥f" next="#šé̥f">
                  <date notBefore="0700" notAfter="0799"/>
                  <form>
                     <pron notation="proprietary">šyé̥f</pron>
                  </form>
                  <!-- early/Proto Old French -->
               </cit>
               <cit type="etymon" xml:id="šé̥f" prev="#šyé̥f" next="šę́f">
                  <date notBefore="1500" notAfter="1650"/>
                  <form>
                     <pron notation="proprietary" xml:lang="frm">šé̥f</pron>
                  </form>
               </cit>
               <date notBefore="1500" notAfter="1650"/>
               <cit type="etymon" xml:id="šę́f" prev="šé̥f">
                  <form>
                     <pron notation="proprietary" xml:lang="frm">šę́f</pron>
                  </form>
               </cit>
               <bibl>Laborderie, N. and Thomasset, C. (1994). Précis de phonétique historique.
                  Paris: Nathan.</bibl>
            </etym>
         </entry>
