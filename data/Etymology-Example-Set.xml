         <!-- METONYMY -->
         <entry xml:id="animal-horse">
            <form type="lemma">
               <orth>kiti</orth>
               <pron notation="ipa">kì.t̪í</pron>
               <gramGrp>
                  <pos>noun</pos>
               </gramGrp>
            </form>
            <sense corresp="http://dbpedia.org/resource/Horse">
               <usg type="dom" corresp="http://dbpedia.org/resource/Animal">Animal</usg>
               <etym type="metonymy">
                  <date notBefore="1517"/>
                  <cit type="etymon">
                     <form>
                        <orth corresp="#animal">kiti</orth>
                     </form>
                     <form>
                        <pron notation="ipa" corresp="#animal">kì.t̪í</pron>
                     </form>
                     <gloss>animal</gloss>
                  </cit>
                  <note>The term comes from the general Mixtepec-Mixtec word for 'animal', it is
                     clearly derrived from the fact that when they encountered horses and the
                     Spanish for the first time (sometime after 1517), they also encountered horses
                     for the first time for which there was no word in the language. They refered to
                     them simply as 'animal' and this use lexicalized in the language.</note>
               </etym>
               <cit type="translation" xml:lang="en">
                  <oRef>horse</oRef>
               </cit>
               <cit type="translation" xml:lang="es">
                  <oRef>caballo</oRef>
               </cit>
            </sense>
         </entry>
         <!-- GRAMMATICALIZATION -->
         <!-- Chronological Ordering of Etymological Attestations -->
         <entry xml:id="besides" xml:lang="en">
            <form type="lemma">
               <orth>besides</orth>
               <gramGrp>
                  <pos>discourseMarker</pos>
               </gramGrp>
            </form>
            <sense><!-- sense info here -->
               <etym type="grammaticalization">
                  <cit type="etymon" xml:id="at-850-950" next="#at-1225-a">
                     <date notBefore="0850" notAfter="0950"/>
                     <form>
                        <orth xml:lang="ang">sidan</orth>
                     </form>
                     <gloss>side</gloss>
                     <gramGrp><!-- inherantly applies to the top level structure -->
                        <pos>locativeNoun</pos>
                     </gramGrp>
                     <cit type="component" corresp="#e1s1">
                        <!-- subtype="head" -->
                        <gramGrp>
                           <pos>noun</pos>
                        </gramGrp>
                     </cit>
                     <cit type="component" corresp="#e1s2">
                        <gramGrp>
                           <case>dative</case>
                        </gramGrp>
                     </cit>
                     <cit type="attestation" xml:lang="ang">
                        <quote>&amp; þonne licge on Da swiDran <oRef>sidan</oRef> gode hwile</quote>
                        <cit type="translation" xml:lang="en">
                           <quote>and then lie on the right side good while</quote>
                        </cit>
                     </cit>
                     <bibl>(850-950 Lacnunga Magicand&amp;Med., p. 120 [HC])</bibl>
                  </cit>
                  <!-- 1225-a is a dative noun; 1225-b is an adverbial in which the dative is used but functions  -->
                  <cit type="etymon" xml:id="at-1225-a" prev="#at-850-950" next="#at-1225-b">
                     <date when="1225"/>
                     <form>
                        <orth xml:lang="ang">bi-siden</orth>
                     </form>
                     <gloss>at his side</gloss>
                     <gramGrp><!-- inherantly applies to the top level structure -->
                        <pos>locativeAdverbial</pos>
                     </gramGrp>
                     <cit type="component" corresp="#e2s1">
                        <gramGrp>
                           <pos>adverb</pos>
                        </gramGrp>
                     </cit>
                     <cit type="component" corresp="#e2s2">
                        <gramGrp>
                           <pos>noun</pos>
                        </gramGrp>
                     </cit>
                     <cit type="component" corresp="#e2s3">
                        <gramGrp>
                           <case>dative</case>
                        </gramGrp>
                     </cit>
                     <cit type="attestation" xml:lang="ang">
                        <quote>His pic he heold <oRef>bi-siden</oRef>
                        </quote>
                        <cit type="translation" xml:lang="en">
                           <quote>He held his staff at his side</quote>
                        </cit>
                     </cit>
                     <bibl>(1225 Lay. Brut 30784 [MED])</bibl>
                  </cit>
                  <cit type="etymon" xml:id="at-1225-b" prev="#at-1225-a" next="#at-c1300">
                     <date when="1225"/>
                     <form>
                        <orth xml:lang="enm">bi-siden</orth>
                     </form>
                     <gloss>in addition</gloss>
                     <gramGrp>
                        <pos>conjunctiveAdverbial</pos>
                     </gramGrp>
                     <cit type="component" corresp="#e3s1">
                        <gramGrp>
                           <pos>preposition</pos>
                        </gramGrp>
                     </cit>
                     <cit type="component" corresp="#e3s2">
                        <gramGrp>
                           <pos>nominalAdposition</pos>
                        </gramGrp>
                     </cit>
                     <cit type="component" corresp="#e3s3">
                        <gramGrp>
                           <case>dative</case>
                        </gramGrp>
                     </cit>
                     <cit type="attestation" xml:lang="ang">
                        <quote>Heo letten forD <oRef>bi-siden</oRef> an oþer folc riden, ten þusend
                           kempen</quote>
                        <cit type="translation" xml:lang="en">
                           <quote>They sent another army forth in addition, 10,000 warriors</quote>
                        </cit>
                     </cit>
                     <bibl>(1225 Lay Brut 5498 [MED])</bibl>
                  </cit>
                  <cit type="etymon" xml:id="at-c1300" prev="#at-1225-b" next="#at-1450">
                     <date cert="medium" when="1300"/>
                     <form>
                        <orth xml:lang="enm">biside</orth>
                     </form>
                     <!-- in the source 'bi-' is represented as (possibly lexicalized?) part of word, functioning as adverb-->
                     <gloss>nearby</gloss>
                     <gramGrp>
                        <pos>locativeAdverbial</pos>
                     </gramGrp>
                     <cit type="attestation" xml:lang="ang">
                        <quote>þe barons astunte wiþoute toun <oRef>biside</oRef> &amp; vaire sende
                           in to þe toun to þe king hor sonde</quote>
                        <cit type="translation" xml:lang="en">
                           <quote>the barons stood outside the town nearby and duly sent in to the
                              town to the king their messenger</quote>
                        </cit>
                     </cit>
                     <bibl>(c. 1300 Robert of Gloucester, p. 748 [HC])</bibl>
                  </cit>
                  <cit type="etymon" xml:id="at-1450" prev="#at-c1300" next="#at-1514-1518">
                     <date notAfter="1449"/>
                     <form>
                        <orth xml:lang="enm">be-syde</orth>
                     </form>
                     <gloss>in addition to</gloss>
                     <gramGrp>
                        <gram>prepositionPhrase</gram>
                        <!-- Traugott: new sense -->
                     </gramGrp>
                     <cit type="component" corresp="#e5s1">
                        <gramGrp>
                           <pos>preposition</pos>
                        </gramGrp>
                     </cit>
                     <cit type="component" corresp="#e5s2">
                        <gramGrp>
                           <pos>noun</pos>
                        </gramGrp>
                     </cit>
                     <cit type="attestation" xml:lang="enm">
                        <quote>..and <oRef>be-syde</oRef> thise ther be in that cerkyl pale spottys,
                           or yewlu ...</quote>
                        <cit type="translation" xml:lang="en">
                           <quote>.. and in addition to this there are in that circle spots that are
                              pale, or yellow...</quote>
                        </cit>
                     </cit>
                     <bibl>(bef. 1450 Metham, p.127 [HC])</bibl>
                  </cit>
                  <!-- Stage I-->
                  <cit type="etymon" xml:id="at-1514-1518" prev="#at-1450" next="#at-1535-1543">
                     <date notBefore="1514" notAfter="1518"/>
                     <form>
                        <orth xml:lang="en">beside</orth>
                     </form>
                     <!-- 14 -18 ys after iso cutoff for middle engl!? need Early Modern iso 639-id -->
                     <gramGrp>
                        <pos>extensionAdverb</pos>
                        <!-- aka 'conjunctiveAdverbial' -->
                     </gramGrp>
                     <cit type="attestation">
                        <!-- early modern english -->
                        <quote>In whiche albeit thei ment as muche honor to hys grace as wealthe to
                           al the realm <oRef ana="OBLQ">beside</oRef>, yet were they not sure howe
                           hys grace woulde take it,...</quote>
                        <!-- @ana="OBLQ" stands for "oblique" usage of item in information structure  -->
                     </cit>
                     <bibl>(1514-18 More, History of Richard III, p. 78)</bibl>
                  </cit>
                  <!-- this next step is actually due to metaphor (domain SPACE > domain DISCOURSE) -->
                  <!-- alternative encoding could have this etymon encased in <etym type="metaphor">; -->
                  <cit type="etymon" xml:id="at-1535-1543" prev="#at-1514-1518" next="#at-1567">
                     <date notBefore="1535" notAfter="1543"/>
                     <form>
                        <orth xml:lang="emodeng">beside</orth>
                     </form>
                     <gramGrp>
                        <pos>extensionAdverb</pos>
                     </gramGrp>
                     <cit type="attestation">
                        <quote>The toune of Chester is chiefly one streate of very meane building yn
                           lenght: ther is <oRef>beside</oRef> a smaul streat or 2. about the
                           chirch; that is collegiatid, .... </quote>
                     </cit>
                     <bibl>(1535-43 Leland, Itinerary I p. 74 [HC])</bibl>
                  </cit>
                  <!-- Stage II: Sentential Adverb -->
                  <!-- alternative encoding could have this etymon formally pointing to the previous sense in <etym type="metaphor">; <sRef> -->
                  <cit type="etymon" xml:id="at-1552-1563" prev="#at-1535-1543" next="#at-1554">
                     <date notBefore="1552" notAfter="1563"/>
                     <form>
                        <orth xml:lang="emodeng">besides</orth>
                     </form>
                     <!--  -->
                     <gramGrp>
                        <pos>sententialAdverb</pos>
                     </gramGrp>
                     <cit type="attestation">
                        <quote>Tolde he not you <seg ana="#Compilmentizer">that</seg>
                           <oRef>besides</oRef> she stole your Cocke that tyde?</quote>
                     </cit>
                     <note>The extensionAdverb signaling extension of a list of referents
                        illustrated in (sense in example <ref target="#at-1535-1543"/>) is
                        presumably the source of the clause-initial, focused IPAdv, which extends
                        the propositional content with additional, non- central material:</note>
                     <bibl>(1552-63 Gammer Gurton, p. 61 [HC])</bibl>
                  </cit>
                  <!-- decision in ordering the former and the next example is arbitrary because the previous is only given a range of dates-->
                  <cit type="etymon" xml:id="at-1554" prev="#at-1552-1563" next="#stage3-26a">
                     <date when="1554"/>
                     <form>
                        <orth xml:lang="emodeng">besides</orth>
                     </form>
                     <!-- should be Early Modern Engl (no iso 639 code) -->
                     <gramGrp>
                        <pos>discourseMarker</pos>
                     </gramGrp>
                     <cit type="attestation">
                        <quote>...<seg ana="#LPeriph">And <oRef>besides</oRef>
                           </seg>, it is very unlike, that </quote>
                     </cit>
                     <bibl>(bef. 1554 Trial Throckmorton I,66.C1 [HC])</bibl>
                  </cit>
                  <cit type="etymon" xml:id="at-1567" prev="#at-1554" next="#at-1619">
                     <date when="1567"/>
                     <form>
                        <orth xml:lang="emodeng">besides</orth>
                     </form>
                     <gramGrp>
                        <pos>extensionAdverb</pos>
                     </gramGrp>
                     <cit type="attestation">
                        <quote>... as I shall aunswere before god, one of them told me
                              <oRef>besides</oRef> that you were godfather to him, and
                           that...</quote>
                     </cit>
                     <bibl>(1567 Harman Caveat p. 40 [HC])</bibl>
                  </cit>
                  <cit type="etymon" xml:id="at-1619" prev="#at-1554" next="#at-1872">
                     <date when="1619"/>
                     <form>
                        <orth xml:lang="en">beside</orth>
                     </form>
                     <gramGrp>
                        <pos>discourseMarker</pos>
                     </gramGrp>
                     <cit type="attestation">
                        <quote>...and <oRef>beside</oRef>, my complexion is so blacke, that...
                        </quote>
                     </cit>
                     <bibl>(1619 Deloney, Jack of Newbury, p.70 [HC])</bibl>
                  </cit>
                  <!-- occurs in left periphery of sentence -->
                  <cit type="etymon" xml:id="at-1872" prev="#at-1619">
                     <date when="1872"/>
                     <form>
                        <orth xml:lang="en">besides</orth>
                     </form>
                     <gramGrp>
                        <pos>discourseMarker</pos>
                     </gramGrp>
                     <cit type="attestation">
                        <quote>... <oRef>besides</oRef> if you put a trusty female at
                           Ravenscroft....</quote>
                     </cit>
                     <bibl>(1872 Amberley Ltrs, p. 513 [CLME])</bibl>
                  </cit>
                  <bibl>(Traugott, 1995)</bibl>
               </etym>
            </sense>
         </entry>



