         <!-- CONTRADICTORY ETYMOLOGICAL ATTESTATIONS/DESCRIPTIONS -->
         <entry xml:id="SCOLD-Skeat">
            <form type="lemma">
               <orth>SCOLD</orth>
            </form>
            <sense>
               <def>to chide, rail at.</def>
               <note>(<lang expand="English">E.</lang>?)</note>
            </sense>
            <etym type="inheritance">
               <cit type="etymon">
                  <lang expand="Middle English">ME.</lang>
                  <form>
                     <orth xml:lang="enm">scolden</orth>
                  </form>
                  <bibl>P. Plowman, B. ii. 81;</bibl>
                  <cit type="example"/>
                  <cit type="etymon">
                     <form>
                        <orth xml:lang="enm">scolde</orth>
